function pad() {
  local string=$1
  local expected_length=$2
  local padchar=$3

  local actual_length=${#string}
  local num_padchars=$[$expected_length - $actual_length]

  if [[ $num_padchars -le 0 ]]; then
    echo -n $string
  else
    for i in $(seq 1 $num_padchars); do
      echo -n $padchar
    done

    echo -n $string
  fi
}

function update() {
  selection=$(cat)

  local codepoints="${emoji_map[$selection]}"
  local result=""

  for codepoint in $codepoints; do
    result="${result}U$(pad $codepoint 8 0)"
  done

  echo $result
}

# execute dmenu and instruct X server to type result
for key in "${!emoji_map[@]}"; do
  echo $key
done | dmenu -f -c -l 50 | update | xargs xdotool key --clearmodifiers
