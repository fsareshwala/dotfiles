#!/usr/bin/env python

# This script calculates the dates for given days of the week from a starting time range to an
# ending time range

from datetime import date
from datetime import timedelta
from enum import Enum

class Weekday(Enum):
    Monday = 0
    Tuesday = 1
    Wednesday = 2
    Thursday = 3
    Friday = 4
    Saturday = 5
    Sunday = 6

start = date(2018, 1, 9)
end = date(2018, 5, 11)

current = start
while current <= end:
    if current.weekday() == Weekday.Tuesday.value or current.weekday() == Weekday.Friday.value:
        print current.strftime('%B %d, %Y')

    current += timedelta(days=1)
