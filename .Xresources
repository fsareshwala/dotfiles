Xcursor.size: 16
! Xft.dpi: 144  # when using a 4k monitor

/* URxvt.font: xft:terminus:size=10 */
URxvt.allowBoldFonts: false
URxvt.bellSuppressTime: 0
URxvt.boldFont:
URxvt.font: xft:Terminess TTF Nerd Font Mono:regular:size=10
URxvt.letterSpace: 1
URxvt.pointerBlank: true
URxvt.saveLines: 65535
URxvt.scrollBar: false
URxvt.scrollTtyKeypress: true
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.url-launcher: /usr/bin/google-chrome

! URxvt.shading: 0 to 99 darkens, 101 to 200 lightens
! URxvt.shading: 15
! URxvt.transparent: true
! URxvt.fading: 75

! extensions configuration
URxvt.perl-lib: /home/fsareshwala/prefix/usr/local/rxvt/perls
URxvt.perl-ext-common: keyboard-select,url-select,clipboard,matcher,resize-font,52-osc
URxvt.keysym.C-i: perl:keyboard-select:activate
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/google-chrome
URxvt.url-select.underline: true
URxvt.keysym.C-minus: resize-font:smaller
URxvt.keysym.C-equal: resize-font:bigger
URxvt.keysym.C-0: resize-font:reset
URxvt.keysym.C-question: resize-font:show
URxvt.resize-font.step: 2
URxvt.matcher.button: 1
URxvt.clipboard.copycmd: xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.clipboard.autocopy: true

! set shift+enter and ctrl+enter to behave like vim expects them to
URxvt.keysym.S-Return:     \033[13;2u
URxvt.keysym.C-Return:     \033[13;5u

URxvt.matcher.pattern.0: https?:\/\/[a-zA-Z0-9-_.\/?&=+:#%;]+
URxvt.matcher.pattern.1: go\/[a-zA-Z0-9-_.\/]+
URxvt.matcher.pattern.2: b\/[0-9]+
URxvt.matcher.pattern.3: doc\/[a-zA-Z0-9-_]+
URxvt.matcher.pattern.4: fxbug.dev\/[0-9]+
URxvt.matcher.pattern.5: fxrev.dev\/[0-9]+

