{
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cc",
    "cpp",
    "md",
    "json",
    "sh"
  ],
  "explorer.icon.enableVimDevicons": false,
  "explorer.datetime.format": "MM/dd/yy HH:mm:ss",
  "explorer.previewAction.onHover": false,
  "explorer.width": 32,
  "explorer.file.hiddenRules": {
    "extensions": [
      "o",
      "a",
      "obj",
      "pyc"
    ],
    "filenames": [],
    "patternMatches": [
      "^\\.",
      "bazel-*"
    ]
  },
  "explorer.toggle": true,
  "explorer.sources": [
    {
      "name": "bookmark",
      "expand": false
    },
    {
      "name": "buffer",
      "expand": false
    },
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.keyMappings.global": {
    "<cr>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "d": "deleteForever",
    "u": "gotoParent",
    "s": "open:vsplit",
    "i": "open:split"
  },
  "languageserver": {
    "ciderlsp": {
      "command": "/google/bin/releases/editor-devtools/ciderlsp",
      "args": [
        "--tooltag=coc-nvim",
        "--noforward_sync_responses"
      ],
      "filetypes": [
        "borg",
        "c",
        "cpp",
        "go",
        "java",
        "proto",
        "python",
        "textproto"
      ]
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
        "c",
        "cpp",
        "objc",
        "objcpp"
      ]
    }
  }
}
